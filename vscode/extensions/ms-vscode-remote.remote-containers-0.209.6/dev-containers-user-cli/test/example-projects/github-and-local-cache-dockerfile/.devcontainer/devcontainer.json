{
	"name": "GitHubAndLocalCacheDockerfile",
	"build": {
		"dockerfile": "Dockerfile",
		"args": { "VARIANT": "focal" }
	},
    "containerEnv": {
        "FROM_DEVCONTAINER": "yes"
    },
	"extensions": [],
	"remoteUser": "vscode",
	"features": {
		"codspace/myfeatures/helloworld": {
			"greeting": "howdy"
		},
		"codspace/myotherfeatures/helloworld": {
			"greeting": "heythere"
		},
		"github-cli": "latest"
	}
}