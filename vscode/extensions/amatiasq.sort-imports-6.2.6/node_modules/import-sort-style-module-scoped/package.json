    "test:watch": "mocha -w --require ts-node/register --recursive \"test/**/*.ts\""
  },
  "author": "Cliff Koh (https://github.com/cliffkoh)",
  "repository": "cliffkoh/import-sort-style-module-scoped",
  "license": "ISC",
  "dependencies": {
    "import-sort-style": "^4.2.0"
  },
  "devDependencies": {
    "@types/chai": "^4.0.0",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8",
    "chai": "^4.0.2",
    "mocha": "^4",
    "ts-node": "^3.0.6",
    "tslint": "^5.4.3",
    "typescript": "^2.3.4"
  }
}
